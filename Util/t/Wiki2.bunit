ShellUtil();

[
    [qw(-realm site validate_realm)] => sub {
	my(undef, $actual) = @_;
	assert_equals(qr{Not a valid.*/Wiki/index}is, $actual->[0]);
	return 1;
    },
    ['validate_all_realms', email('site-reports')] => qr{
        fourem:.\d+.errors
	.*
        site-help:.2.errors
	.*
        site:.9.errors
    }xis,
];
