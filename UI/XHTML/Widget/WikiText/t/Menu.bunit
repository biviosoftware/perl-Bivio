WikiText();
my($t1) = class('Type.WikiDataName')->to_absolute('T1.bmenu');
model('RealmFile')->delete({path => $t1});
model('RealmFile')->create_with_content({path => $t1}, \(<<'EOF'));
Label,Link,Class
Index
Home,/
Login,/pub/login
MyPage,^MyPage
Document,^Document,pdf
EOF
[
    [{
	value => '@b-menu T1',
	is_public => 0,
    }] => qr{@{[join('.*', qw(
       class="bmenu"
       "/fourem/wiki/Index" Index
       "/" Home
       "/pub/login" Login
       "/fourem/wiki/MyPage" MyPage
    )).qq(.*class="pdf[^"]*".*"/fourem/wiki/Document".*Document)]}},
    [{
	value => '@b-menu value=T1',
	is_public => 0,
    }] => qr{@{[join('.*', qw(
       class="bmenu"
       "/fourem/wiki/Index" Index
    ))]}},
];
