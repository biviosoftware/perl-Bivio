    '@center' => qq{},
    '@a' => qq{<p class="b_prose"><a></a></p>},
    '@a href=/x y' => qq{<p class="b_prose"><a href="/x">y</a></p>},
    '@a href=x y' => qr{<p class="b_prose"><a href="/bunit_wiki/\w+/x">y</a></p>},
    '@a href=^x y' => qr{<p class="b_prose"><a href="/bunit_wiki/\w+/x">y</a></p>},
