FormModel();
require 'Blog.PL';
[
    map({
	my($x) = $_;
	[{
	    title => "t$x",
	    body => "b$x",
	}] => [{
	    'Model.RealmFile' => {
		path => sub {
		    my($rf) = req()->get('Model.RealmFile');
		    my($c) = $rf->get_content;
		    assert_equals("\@h3 t$x\nb$x", $$c);
		    return $rf->get('path');
		},
	    },
	}];
     } 1..5),
];
