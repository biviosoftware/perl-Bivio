my($x) = 0;
[
    [[qw(3 1 2)]] => [
	map_iterate => [
	    [sub {"!$_[0]!"}] => [[qw(!3! !1! !2!)]],
	],
	do_iterate => [
	    [sub {$x++; return shift == 3 ? 1 : 0}] => sub {
		assert_equals(2, $x);
		return 1;
	    },
	],
	as_string => 'StringArray[3, 1, 2]',
	equals => [
	    [class()->new([qw(3 1 2)])] => 1,
	    [undef] => 0,
	],
	is_specified => 1,
    ],
    [[]] => [
	is_specified => 0,
    ],
];
