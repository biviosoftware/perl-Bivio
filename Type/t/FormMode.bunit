Request();
req()->set_realm_and_user(qw(demo demo));
[
    class() => [
	setup_by_list_this => [
	    map({
		my($mode) = $_;
		sub {
		    req()->put(query => {this => req('auth_id')})
			if $mode eq 'EDIT';
		    return [model('UserRealmList'), 'RealmOwner'];
		} => [class()->$mode()],
	    } qw(CREATE EDIT)),
        ],
    ],
];
